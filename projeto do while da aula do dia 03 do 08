
nome_do_usuario = input("digite seu nome: ")
senha_do_ususario = input("digite sua senha: ")

while(nome_do_usuario == senha_do_ususario):
        print("A senha não pode ser igual ao nome!")
        nome_do_usuario = input("digite seu nome: ")
        senha_do_ususario = input("digite sua senha: ")

print("usuário logado com sucesso!!!")

